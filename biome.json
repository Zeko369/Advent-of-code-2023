{
	"$schema": "https://biomejs.dev/schemas/1.4.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"formatter": {
		"lineWidth": 120
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,

			"style": {
				"useConst": "off",
				"noUselessElse": "off"
			},

			"correctness": {
				"noUnreachable": "off"
			}
		}
	}
}
